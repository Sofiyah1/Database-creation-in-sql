Info space community BIodata creation

CREATE DATABASE [INFOSPACE_DAO];
creating Tables
USE [INFOSPACE_DAO]
CREATE TABLE [INFOSPACE_MODS](MEMBERS_ID INT PRIMARY KEY,
								NAME VARCHAR(50),
								AGE INT,
								GENDER NCHAR,
								MARITALSTATUS NCHAR,
								YEARS_IN_WEB3 INT)

INSERT INTO [INFOSPACE_MODS](MEMBERS_ID,
								NAME ,
								AGE ,
								GENDER ,
								MARITALSTATUS ,
								YEARS_IN_WEB3 )
VALUES(11,'WILDCHILD',25,'M','S',2)
this step was repeated for all members by replacing the details in the value()
     
CREATE TABLE [MEMBERS_BIODATA](MEMBERS_ID INT PRIMARY KEY,
								NAME VARCHAR(50),
								AGE INT,
								GENDER NCHAR,
								MARITALSTATUS NCHAR,
								YEARS_IN_WEB3 INT,
								HIGHESTEARNINGS VARCHAR(20),
								PREFERED_SOCIAL_PLATFORM VARCHAR(20))
INSERT INTO [MEMBERS_BIODATA](MEMBERS_ID ,
								NAME ,
								AGE ,
								GENDER ,
								MARITALSTATUS,
								YEARS_IN_WEB3 ,
								HIGHESTEARNINGS,
								PREFERED_SOCIAL_PLATFORM)
VALUES(34,'Oladimeji',26,'M','S',4,'350','Twitter')
checking the tables content
SELECT *
FROM[dbo].[INFOSPACE_MODS]

SELECT*
FROM[MEMBERS_BIODATA]



--QUERY TO EXTRACT ALL FEMALES FROM [MEMBERS_BIODATA]
SELECT*
FROM[MEMBERS_BIODATA]
WHERE [GENDER]='F'

--QUERY TO SELECT MEMBERS WHO PREFER TWITTER AND WHOSE EARNINGS ARE ABOVE  $500
SELECT *
FROM[MEMBERS_BIODATA]
WHERE [PREFERED_SOCIAL_PLATFORM]='TWITTER'
AND [HIGHESTEARNINGS]>500

--QUERY TO SEGREGATE BASED ON AGE GROUP
--19-26'GENZ'
--26-40 'MILLENIAL'

SELECT [MEMBERS_ID],
       [NAME],
	   [AGE],
	   [GENDER],
	   [HIGHESTEARNINGS],
 CASE
      WHEN[AGE] BETWEEN '19' AND 25 THEN 'GENZ'
	  WHEN[AGE] BETWEEN '26' AND 35 THEN 'MILLENIAL'
	  ELSE 'YOUNGSTAR'
  END AS AGE_GROUP
FROM[MEMBERS_BIODATA]

